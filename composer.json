{
    "name": "matthewbdaly/laravel-authority",
    "description": "Laravel permissions package",
    "keywords": ["permissions","laravel","roles","authority"],
    "type": "library",
    "require-dev": {
        "orchestra/testbench": "^3.5",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.4",
        "squizlabs/php_codesniffer": "^3.1",
        "satooshi/php-coveralls": "^1.0",
        "psy/psysh": "^0.8.14"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matthew Daly",
            "email": "matthewbdaly@gmail.com"
        }
    ],
    "require": {
        "matthewbdaly/laravel-repositories": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Matthewbdaly\\LaravelAuthority\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Matthewbdaly\\LaravelAuthority\\Providers\\AuthorityServiceProvider"
            ]
        }
    }
}
